<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="true"></constant> 
	<package name="userpackage"  extends="struts-default">
		<action name="getAll" class="com.qhit.t5.action.UserAction">
			<result name="list">/index.jsp</result>
		</action>
		<action name="add" class="com.qhit.t5.action.UserAction">
			<result name="tolist" type="redirectAction">getAll</result>
		</action>
			
	</package>


</struts>
	