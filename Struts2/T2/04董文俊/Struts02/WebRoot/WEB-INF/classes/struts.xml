<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="main" extends="struts-default">
<!--     	定义拦截器 -->
	<interceptors>
		<interceptor name="myinterceptor" class="com.kd.struts02.Interceptor.MyInterceptor"></interceptor>
	</interceptors>
    <!-- name必须与表单提交中的action里面的值一样 class必须是Action类的全称 -->
       <action name="login"  class="com.kd.struts02.action.LoginAction">
       		<interceptor-ref name="myinterceptor"/>
       		<interceptor-ref name="timer"/>
    		<result name="success">jsp/MyJsp.jsp</result>
		</action>
    </package>
</struts>