		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="t_classInfo" schema="dbo" catalog="db_exam"
	class="com.qhit.lh.gr3.sakura.exam.bjgl.bean.ClassInfo">
